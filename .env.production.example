# Production Environment Variables Template
# Copy this to .env.production and fill in your values

# Gemini API Key (Backend)
GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Configuration (Frontend)
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=kodi-bot-7.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=kodi-bot-7
REACT_APP_FIREBASE_STORAGE_BUCKET=kodi-bot-7.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Backend API URL (will be set after deploying backend to Render)
REACT_APP_API_URL=https://kodi-backend.onrender.com
